ionary-mockup.png'\nimport img2 from '../project-media/Bit Logo/building-mockup.png'\nimport pdf from '../project-media/Bit Logo/BitGroupBrandDesign.pdf'\n\nfunction BitLogo() {\n    return(\n        <>\n            <div className=\"divider\"></div>\n            <p>To see my other designs and process, click <a className=\"link-color\" href={pdf} target=\"_blank\">here</a>.</p>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img1} className=\"project-img\"></img>\n                </div>\n            </div>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img2} className=\"project-img\"></img>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default BitLogo;","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/Resume.js",["242","243","244","245","246","247","248","249","250","251","252","253","254","255","256","257","258","259","260","261","262"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/ProjectOverview.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/DuquesneWebsite.js",["263"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img1 from '../project-media/Duquesne Website/duquesne-site.png'\n\nfunction DuquesneWebsite() {\n    return(\n        <>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img1} className=\"project-img\"></img>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default DuquesneWebsite;","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/DFAWebsite.js",["264"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/TextileWork.js",["265","266","267","268"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img1 from '../project-media/Textile Work/sesame-hat.jpeg'\nimport img2 from '../project-media/Textile Work/embroidery-gogh-lilies.jpeg'\nimport img3 from '../project-media/Textile Work/weaving-back.jpeg'\nimport img4 from '../project-media/Textile Work/weaving-front.jpeg'\n\n\nfunction TextileWork() {\n    return(\n        <>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img3} className=\"project-img\"></img>\n                </div>\n                <div className=\"divider\"></div>\n                <div className=\"project-media\">\n                    <img src={img4} className=\"project-img\"></img>\n                </div>\n            </div>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img1} className=\"project-img\"></img>\n                </div>\n                <div className=\"divider\"></div>\n                <div className=\"project-media\">\n                    <img src={img2} className=\"project-img\"></img>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default TextileWork;\n","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/TwentyFour.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/SentenceMosaics.js",["269"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img from '../project-media/Sentence Mosaics/sentence-mosaics.png'\n\nfunction SentenceMosaics() {\n    return(\n        <>\n            <div className=\"project-media-container\">\n                <img src={img} className=\"project-media\"></img>\n            </div>\n        </>\n    )\n}\n\nexport default SentenceMosaics;","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/Art.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/project-media/Art/Cards.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/Footer/Footer.js",["270","271","272","273"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport './Footer.css'\n\nimport logo from '../data/efuLogo.png'\n\nfunction Footer() {\n\n    const scrollToTop = () => {\n\t\twindow.scrollTo({top: 0, behavior: 'smooth'});\n\t};\n\n    return (\n        <div className='footer-container'>\n            <section className=\"social-media\">\n                <div className=\"social-media-wrap\">\n                    <div className=\"footer-logo\">\n                        <Link to='/' className=\"social-logo\" onClick={scrollToTop}>\n                            <img src={logo} className='navbar-logo-img'></img>\n                        </Link>\n                    </div>\n                    <small className=\"website-rights\">Made with React, 2021</small>\n                    <div className=\"social-icons\">\n                        <a \n                            className=\"social-icon-link linkedIn\"\n                            target='_blank'\n                            href='https://www.linkedin.com/in/erica-fu-15b3a61b9/'\n                        >\n                            <i className=\"fab fa-linkedin\"></i>\n                        </a>\n                        <a \n                            className=\"social-icon-link github\"\n                            target='_blank'\n                            href='https://github.com/erica-w-fu'\n                        >\n                            <i className=\"fab fa-github\"></i>\n                        </a>\n                        <a \n                            className=\"social-icon-link instagram\"\n                            target='_blank'\n                            href='https://www.instagram.com/ericacreatesss/'\n                        >\n                            <i className=\"fab fa-instagram\"></i>\n                        </a>\n                        {/* <a \n                            className=\"social-icon-link facebook\"\n                            target='_blank'\n                            href='https://www.facebook.com/erica.fu.524'\n                        >\n                            <i className=\"fab fa-facebook\"></i>\n                        </a> */}\n                    </div>\n                </div>\n            </section>\n        </div>\n    )\n}\n\nexport default Footer\n","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ResumeSection/ResumeSections.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/FeaturedContent/FeaturedContent.js",[],["274","275"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/TwentyFour.js",["276"],"import React from \"react\";\nimport FeaturedIntro from \"../../FeaturedIntro/FeaturedIntro\";\nimport Section from '../../FeaturedContent/FeaturedContent.js';\n\nimport wireframe from './media/24-wireframes.jpg';\nimport demo from './media/24-demo.mp4';\n\nfunction TwentyFour() {\n    return(\n        <>\n            <FeaturedIntro\n                key=\"twentyfour\"\n                component=\"twentyfour\"\n                title=\"24\"\n                date=\"June 2019 - January 2021\"\n                summary=\"Created a mobile version of my brother’s favorite card game, 24, first evaluating what features made the physical card game fun and incorporating them into the digital version.\"\n                responsibilities=\"Wireframing, Research, Mobile App Development \"\n                tools=\"Xcode\\nSwift\"\n                imgType=\"uncropped\"\n                link=\"\"\n                team=\"\"\n            />\n            <div className=\"divider\"></div>\n            <div className=\"divider\"></div>\n            <p>Used the human-centered design process taught by Design For America to develop this project.</p>\n            <Section \n                title=\"Identify\"\n            >\n                <h6>How can I convert the physical card game, 24, into a digital form for my family?</h6>\n                <small>\n                    To play 24, players take four cards from the deck and use simple arithmetic to get a solution of 24. This game is commonly played and enjoyed by my family.\n                </small>\n            </Section>\n            <Section \n                title=\"Immerse\"\n            >\n                <h6>Interviews</h6>\n                <small>\n                    interviewed my family to gain insights on what the digital game should include\n                </small>\n                <ul>\n                    <li>competitiveness makes the game fun</li>\n                    <li>physical game includes slapping and pointing to declare the answer</li>\n                    <li>the game stimulates the brain since there is math involved</li>\n                </ul>\n                <br></br>\n                <h6>Competitive analysis</h6>\n                <small>\n                    tested other 24 apps on the app store to understand what people have already made\n                </small>\n                <ul>\n                    <li>apps all use the same mechanism to solve the numbers</li>\n                    <li>apps do not include any multiplayer functions</li>\n                    <li>reviews point out that apps sometime gives unsolvable problems</li>\n                </ul>\n            </Section>\n            <Section \n                title=\"Reframe\"\n            >\n                <h6><div className=\"unbolded\">How can I convert the physical card game, 24, into a digital form for my family </div> \n                    so that the competitve and brain stimulating aspects of the game remain?</h6>\n            </Section>\n            <Section \n                title=\"Ideate\"\n            >\n                <p>Created wireframes to map out user interactions with the interface and with other players</p>\n                <br></br>\n                <br></br>\n                <img src={ wireframe } className=\"featured-content-media\"></img>\n            </Section>\n            <Section \n                title=\"Create\"\n            >\n                <video width=\"30%\" controls >\n                    <source src={demo} type=\"video/mp4\"/>\n                </video>\n                \n            </Section>\n            <Section \n                title=\"Test\"\n            >\n                <h6>User testing</h6>\n                <small>\n                    conducted user tests with my family\n                </small>\n                <ul>\n                    <li>found that buttons were intuitive and understandable</li>\n                    <li>family really appreciated the algorithm that displayed potential solutions</li>\n                </ul>\n                <br></br>\n                <h6>Next steps</h6>\n                <small>\n                    how I hope to expand this project\n                </small>\n                <ul>\n                    <li>implement full multiplayer functionalities</li>\n                    <li>deploy on the app store for anyone to play</li>\n                </ul>\n            </Section>\n        </>\n    )\n}\n\nexport default TwentyFour;",["277","278"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/CrossCountryConnections.js",["279","280"],"import React from \"react\";\nimport FeaturedIntro from \"../../FeaturedIntro/FeaturedIntro\";\nimport Section from '../../FeaturedContent/FeaturedContent.js';\n\nimport physical from './media/CCC-physical.mp4';\nimport digital from './media/CCC-digital.mp4';\nimport threeDee from './media/CCC-3D.png';\nimport final from './media/CCC-final.jpeg';\nimport finalDemo from './media/CCC-finalDemo.mp4';\n\nfunction CrossCountryConnections() {\n\n    return(\n        <>\n            <FeaturedIntro\n                key=\"ccc\"\n                component=\"ccc\"\n                title=\"Cross Country Connections\"\n                date=\"May 2021 - present\"\n                summary=\"Constructed installations to explore unique remote communication methods through tactile interactions with a large canvas.\\n\\n \n                    Our team of three students is funded by a Small Undergraduate Research Grant from CMU and advised by Physical Computing instructor Robert Zacharias.\"\n                responsibilities=\"Research, Physical Prototyping, Digital Prototyping, User Testing, Software, Hardware, 3D Modeling\"\n                tools=\"Arduino\\n\n                Raspberry Pi\\n\n                Unity\\n\n                Fusion 360\\n\n                Python\"\n                imgType=\"uncropped\"\n                link=\"https://crosscountryconnections.github.io/index.html\"\n                linkTitle=\"Visit Website\"\n                team=\"Michelle Dang, Jennifer Zhang, Robert Zacharias\"\n            />\n            <div className=\"divider\"></div>\n            <p>After taking Physical Computing and unlocking the powers of microprocessors, my friend and I decided to create an installation with our newfound skills. We knew that we both wanted to make something big and interactive, but we quickly realized that she would be staying at Pittsburgh while I would go hope to the Bay Area over the summer. \n                However, we decided to take advantage of our different locations, eventually choosing to create two large canvases that would sync across the country, giving us the ability to communicate through LEDs.</p>\n            <br></br>\n            <p>Our team made various physical and digital prototypes with different materials and methods to test out ideas before building the final installation.</p>\n            <Section \n                title=\"Physical Protoype\"\n            >\n                <p>Synchronized two physical prototypes</p>\n                <br></br>\n                <br></br>\n                <div className=\"featured-col-container\">\n                    <div className=\"featured-col\">\n                        <h6>Methods</h6>\n                        <ul>\n                            <li>connected one board (in California) to the other (in Pittsburgh)</li>\n                            <li>created a small version of our intended interaction to determine how to encourage connections between two remote users</li>\n                        </ul>\n                    </div>\n                    <div className=\"featured-col\">\n                        <h6>Software</h6>\n                        <ul>\n                            <li>Arduino</li>\n                            <li>Python</li>\n                        </ul>\n                        <br></br>\n                        <h6>Materials</h6>\n                        <ul>\n                            <li>tactile buttons</li>\n                            <li>LED lights</li>\n                            <li>wires</li>\n                            <li>cardboard and tape</li>\n                        </ul>\n                    </div>\n                </div>\n                <br></br>\n                <video width=\"100%\" controls>\n                    <source src={physical} type=\"video/mp4\"/>\n                </video>\n            </Section>\n            <Section \n                title=\"Digital Protoype\"\n            >\n                <p>Synchronized the physical prototype with a digital prototype</p>\n                <br></br>\n                <br></br>\n                <div className=\"featured-col-container\">\n                    <div className=\"featured-col\">\n                        <h6>Methods</h6>\n                        <ul>\n                            <li>digitally simulated the button pressing mechanism to capture interactions</li>\n                            <li>determined what size matrix would allow for creativity</li>\n                        </ul>\n                    </div>\n                    <div className=\"featured-col\">\n                        <h6>Software</h6>\n                        <ul>\n                            <li>Unity</li>\n                            <li>Arduino</li>\n                            <li>Python</li>\n                        </ul>\n                    </div>\n                </div>\n                <br></br>\n                <video width=\"100%\" controls>\n                    <source src={digital} type=\"video/mp4\"/>\n                </video>\n            </Section>\n            <Section \n                title=\"3D Protoype\"\n            >\n                <p>Planned for the final build</p>\n                <br></br>\n                <br></br>\n                <div className=\"featured-col-container\">\n                    <div className=\"featured-col\">\n                        <h6>Methods</h6>\n                        <ul>\n                            <li>constructed schematic of cuts and engravings for kaser cutting</li>\n                            <li>modeled how all the boards and components would fit together</li>\n                        </ul>\n                    </div>\n                    <div className=\"featured-col\">\n                        <h6>Software</h6>\n                        <ul>\n                            <li>Fusion 360</li>\n                        </ul>\n                    </div>\n                </div>\n                <br></br>\n                <img src={ threeDee } className=\"featured-content-media\"></img>\n            </Section>\n            <Section \n                title=\"Final Installation\"\n            >\n                <p>Used findings from all the previous prototypes to create a final installation</p>\n                <br></br>\n                <br></br>\n                <div className=\"featured-col-container\">\n                    <div className=\"featured-col\">\n                        <h6>Methods</h6>\n                        <ul>\n                            <li>constructed by laser cutting acrylic, creating a button matrix, soldering wires, and more</li>\n                            <li>used same methods as physical prototype to send signals and light board</li>\n                            <li>used same methods as physical prototype to send signals and light board</li>\n                        </ul>\n                    </div>\n                    <div className=\"featured-col\">\n                        <h6>Software</h6>\n                        <ul>\n                            <li>Arduino</li>\n                            <li>Raspberry Pi</li>\n                            <li>Python</li>\n                        </ul>\n                        <br></br>\n                        <h6>Materials</h6>\n                        <ul>\n                            <li>frosted acrylic</li>\n                            <li>tactile buttons</li>\n                            <li>wires</li>\n                            <li>wood</li>\n                            <li>etc.</li>\n                        </ul>\n                    </div>\n                </div>\n                <br></br>\n                <video width=\"100%\" controls>\n                    <source src={finalDemo} type=\"video/mp4\" />\n                </video>\n                <img src={ final } className=\"featured-content-media\"></img>\n            </Section>\n        </>\n    )\n}\n\nexport default CrossCountryConnections","/Users/ericafu/CMU/erica-w-fu.github.io/src/index.js",[],["281","282"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/About.js",["283","284","285","286"],"import React from 'react';\nimport '../../App.css';\nimport './Pages.css';\n\nimport Footer from '../Footer/Footer.js'\n\nimport hello from './aboutMedia/hello.png'\nimport doodles from './aboutMedia/doodles.png'\nimport profile from './aboutMedia/profile.png'\n\nimpor