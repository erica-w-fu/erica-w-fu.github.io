.io/src/components/ProjectSection/project-media/Art/Cards.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/Footer/Footer.js",["234","235","236","237"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport './Footer.css'\n\nimport logo from '../data/efuLogo.png'\n\nfunction Footer() {\n\n    const scrollToTop = () => {\n\t\twindow.scrollTo({top: 0, behavior: 'smooth'});\n\t};\n\n    return (\n        <div className='footer-container'>\n            <section className=\"social-media\">\n                <div className=\"social-media-wrap\">\n                    <div className=\"footer-logo\">\n                        <Link to='/erica-w-fu' className=\"social-logo\" onClick={scrollToTop}>\n                            <img src={logo} className='navbar-logo-img'></img>\n                        </Link>\n                    </div>\n                    <small className=\"website-rights\">Created with React, 2021</small>\n                    <div className=\"social-icons\">\n                        <a \n                            className=\"social-icon-link linkedIn\"\n                            target='_blank'\n                            href='https://www.linkedin.com/in/erica-fu-15b3a61b9/'\n                        >\n                            <i className=\"fab fa-linkedin\"></i>\n                        </a>\n                        <a \n                            className=\"social-icon-link github\"\n                            target='_blank'\n                            href='https://github.com/erica-w-fu'\n                        >\n                            <i className=\"fab fa-github\"></i>\n                        </a>\n                        <a \n                            className=\"social-icon-link instagram\"\n                            target='_blank'\n                            href='https://www.instagram.com/ericacreatesss/'\n                        >\n                            <i className=\"fab fa-instagram\"></i>\n                        </a>\n                        {/* <a \n                            className=\"social-icon-link facebook\"\n                            target='_blank'\n                            href='https://www.facebook.com/erica.fu.524'\n                        >\n                            <i className=\"fab fa-facebook\"></i>\n                        </a> */}\n                    </div>\n                </div>\n            </section>\n        </div>\n    )\n}\n\nexport default Footer\n","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ResumeSection/ResumeSections.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/FeaturedContent/FeaturedContent.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/TwentyFour.js",["238"],"import React from \"react\";\nimport FeaturedIntro from \"../../FeaturedIntro/FeaturedIntro\";\nimport Section from '../../FeaturedContent/FeaturedContent.js';\n\nimport wireframe from './media/24-wireframes.jpg';\nimport demo from './media/24-demo.mp4';\n\nfunction TwentyFour() {\n    return(\n        <>\n            <FeaturedIntro\n                key=\"twenty_four\"\n                title=\"24\"\n                date=\"June 2019 - January 2021\"\n                summary=\"Created a mobile version of my brother’s favorite card game, 24, first evaluating what features made the physical card game fun and incorporating them into the digital version.\"\n                responsibilities=\"Wireframing, Research, Mobile App Development \"\n                tools=\"xCode\\nSwift\"\n                img=\"twenty_four\"\n                imgType=\"featured-img-uncropped\"\n                link=\"\"\n            />\n            <div className=\"divider\"></div>\n            <div className=\"divider\"></div>\n            <h7>Used the human-centered design process taught by Design For America to develop this project.</h7>\n            <Section \n                title=\"Identify\"\n            >\n                <h6>How can I convert the physical card game, 24, into a digital form for my family?</h6>\n                <p>\n                    To play 24, players take four cards from the deck and use simple arithmetic to get a solution of 24. This game is commonly played and enjoyed by my family.\n                </p>\n            </Section>\n            <Section \n                title=\"Immerse\"\n            >\n                <h6>Interviews</h6>\n                <p>\n                    intereviewed my family to gain insights on what the digital game should include\n                </p>\n                <ul>\n                    <li>competitiveness makes the game fun</li>\n                    <li>physical game includes slapping and pointing to declare the answer</li>\n                    <li>the game stimulates the brain since there is math involved</li>\n                </ul>\n                <br></br>\n                <h6>Competitive analysis</h6>\n                <p>\n                    tested other 24 apps on the app store to understand what people have already made\n                </p>\n                <ul>\n                    <li>apps all use the same mechanism to solve the numbers</li>\n                    <li>apps do not include any multiplayer functions</li>\n                    <li>reviews point out that apps sometime gives unsolvable problems</li>\n                </ul>\n            </Section>\n            <Section \n                title=\"Reframe\"\n            >\n                <h6><div className=\"unbolded\">How can I convert the physical card game, 24, into a digital form for my family </div> \n                    so that the competitve and brain stimulating aspects of the game remain?</h6>\n            </Section>\n            <Section \n                title=\"Ideate\"\n            >\n                <h7>Created wireframes to map out user interactions with the interface and with other players</h7>\n                <img src={ wireframe } className=\"featured-content-media\"></img>\n            </Section>\n            <Section \n                title=\"Create\"\n            >\n                <video width=\"30%\" controls >\n                    <source src={demo} type=\"video/mp4\"/>\n                </video>\n                \n            </Section>\n            <Section \n                title=\"Test\"\n            >\n                <h6>User testing</h6>\n                <p>\n                    conducted user tests with my family\n                </p>\n                <ul>\n                    <li>found that buttons were intuitive and understandable</li>\n                    <li>family really appreciated the algorithm that displayed potential solutions</li>\n                </ul>\n                <br></br>\n                <h6>Next steps</h6>\n                <p>\n                    how I hope to expand this project\n                </p>\n                <ul>\n                    <li>implement full multiplayer functionalities</li>\n                    <li>deploy on the app store for anyone to play</li>\n                </ul>\n            </Section>\n        </>\n    )\n}\n\nexport default TwentyFour;","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/CrossCountryConnections.js",["239","240"],{"ruleId":"241","severity":1,"message":"242","line":9,"column":8,"nodeType":"243","messageId":"244","endLine":9,"endColumn":23},{"ruleId":"241","severity":1,"message":"245","line":10,"column":8,"nodeType":"243","messageId":"244","endLine":10,"endColumn":17},{"ruleId":"246","replacedBy":"247"},{"ruleId":"248","replacedBy":"249"},{"ruleId":"241","severity":1,"message":"250","line":125,"column":16,"nodeType":"243","messageId":"244","endLine":125,"endColumn":24},{"ruleId":"246","replacedBy":"251"},{"ruleId":"248","replacedBy":"252"},{"ruleId":"253","severity":1,"message":"254","line":72,"column":17,"nodeType":"255","endLine":72,"endColumn":79},{"ruleId":"253","severity":