e.log(data);\n\n  return( \n    <>\n      <div className=\"gallery-container-two\">\n        {\n          data.map((item) => {\n            return <div className=\"gallery-two\">\n              {/* <Link to={`/Projects/${item.Component}`}> */}\n                <Card\n                  key={ item.Component }\n                  component={ item.Component }\n                  title={ item.Title }\n                  dates={ item.Dates }\n                  summary={ item.Summary }\n                  link={ item.Link }\n                  linkTitle={ item.LinkTitle }\n                  tags={ item.Tags }\n                  responsibilities={ item.Responsibilities }\n                  tools={ item.Tools }\n                  team={ item.Team }\n                  imgType={ item.ImgType }\n                />\n            </div>\n          })\n        }\n      </div>\n      <Footer />\n    </>\n  );\n}\n\nexport default Projects;\n\n\n",["183","184"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/pages/FeaturedProjects/ThirtyDay.js",["185","186","187"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectSections.js",["188","189"],"import React from \"react\";\nimport './ProjectSection.css';\n\nimport ThirtyDayAdulting from './ProjectMediaComponents/ThirtyDayAdulting.js';\nimport TwentyFour from './ProjectMediaComponents/TwentyFour.js';\nimport SentenceMosaics from './ProjectMediaComponents/SentenceMosaics.js';\nimport DFAWebsite from './ProjectMediaComponents/DFAWebsite.js';\nimport DuquesneWebsite from './ProjectMediaComponents/DuquesneWebsite.js';\nimport Multitask from './ProjectMediaComponents/Multitask.js';\nimport Fish from './ProjectMediaComponents/Fish.js';\nimport BitLogo from './ProjectMediaComponents/BitLogo.js';\nimport CommunicationDesign from './ProjectMediaComponents/CommunicationDesign.js'\nimport Microsite from './ProjectMediaComponents/Microsite.js'\nimport Art from './ProjectMediaComponents/Art.js'\nimport CrossCountryCommunications from  './ProjectMediaComponents/CrossCountryConnections.js'\nimport AcrylicPourPaintingPlatform from  './ProjectMediaComponents/AcrylicPourPaintingPlatform.js'\nimport TextileWork from  './ProjectMediaComponents/TextileWork.js'\n\nfunction ProjectSections(props) {\n    \n    const projectId = props.component.toString().trim();\n\n    return (\n        <div className='project-card'>\n            <div className=\"one-line\">\n                <h3 className=\"one-line-item\">{ props.title }</h3>\n                <h5 className=\"one-line-item project-dates\">{ props.dates }</h5>\n            </div>\n            <p className=\"summary\">{ props.body }</p>\n            {\n                props.link === '' \n                ? <></>\n                : <p>For more information, click <a className=\"link-color underline\" href={ props.link } target=\"_blank\">here</a>.</p> \n            }\n\n            <Project id={ projectId }/>\n            \n        </div>\n    );\n}\n\nfunction importAll(r) {\n    return r.keys().map(r);\n}\n\nconst Project = ({ id }) => {\n    switch (id) {\n        case \"thirty_day_adulting\":\n            return <ThirtyDayAdulting />;\n        case \"twentyfour\":\n            return <TwentyFour />;\n        case \"sentence_mosaics\":\n            return <SentenceMosaics />;\n        case \"dfa_website\":\n            return <DFAWebsite />;\n        case \"duquesne_incline\":\n            return <DuquesneWebsite />;\n        case \"multitask\":\n            return <Multitask />;\n        case \"fish\":\n            return <Fish />;\n        case \"bit_logo\":\n            return <BitLogo />;\n        case \"communication_design\":\n            return <CommunicationDesign />;\n        case \"microsite\":\n            return <Microsite />;\n        case \"art\":\n            return <Art />;\n        case \"ccc\":\n            return <CrossCountryCommunications />;\n        case \"acrylic_pour_painting_platform\":\n            return<AcrylicPourPaintingPlatform />;\n        case \"textile_work\":\n            return <TextileWork />;\n        default:\n            return <p>{id}</p>;\n    }\n};\n\nexport default ProjectSections;\n",["190","191"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/Multitask.js",[],["192","193"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/Fish.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/CrossCountryConnections.js",["194","195"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img1 from '../project-media/Cross Country Connections/ccc-erica.jpeg'\nimport img2 from '../project-media/Cross Country Connections/ccc-mother.jpeg'\n\nfunction CrossCountryCommunications() {\n    return(\n        <>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img1} className=\"project-img\"></img>\n                </div>\n            </div>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img2} className=\"project-img\"></img>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default CrossCountryCommunications;","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/CommunicationDesign.js",["196","197","198"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img1 from '../project-media/Communication Design Fundamentals/gestalt-1.png'\nimport img2 from '../project-media/Communication Design Fundamentals/gestalt-2.png'\n\nfunction CommunicationDesign() {\n    return(\n        <>\n            <div className=\"divider\"></div>\n            <h4>Practicing Form and Composition</h4>\n            <div className=\"divider\"></div>\n            <p>Created for my Communication Design Fundamentals course, I used squares while considering negative space to convey different emotions.</p>\n            <p>To see my process, click <a className=\"link-color underline\" href=\"https://medium.com/@efu_51933/project-2-form-composition-9dcb50da3ac8\" target=\"_blank\">here</a>.</p> \n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img1} className=\"project-img\"></img>\n                </div>\n            </div>\n            <div className=\"project-media-container\">\n                <div className=\"project-media\">\n                    <img src={img2} className=\"project-img\"></img>\n                </div>\n            </div>\n        </>\n    )\n}\n\nexport default CommunicationDesign;",["199","200"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/AcrylicPourPaintingPlatform.js",["201"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport video from '../project-media/Acrylic Pour Painting Platform/acrylic-pour-demo.mov'\nimport img from '../project-media/Acrylic Pour Painting Platform/final-product.jpg'\n\n\nfunction AcrylicPourPaintingPlatform() {\n    return(\n        <>\n            <div className=\"project-media-container\">\n                <video width=\"41%\" controls >\n                    <source src={video} type=\"video/mp4\"/>\n                </video>\n                <div className=\"divider\"></div>\n                <img src={img} className=\"project-media\" style={{ width: \"35%\" }}></img>\n            </div>\n        </>\n    )\n}\n\nexport default AcrylicPourPaintingPlatform;",["202","203"],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/project-media/Art/CardItem.js",["204"],"import React from 'react'\nimport { Link } from 'react-router-dom'\n\nfunction CardItem(props) {\n    return (\n        <>\n            <li className='cards__item'> \n         