      : <p>For more information, click <a className=\"link-color underline\" href={ props.link } target=\"_blank\">here</a>.</p> \n            }\n\n            <Project id={ projectId }/>\n            \n        </div>\n    );\n}\n\nfunction importAll(r) {\n    return r.keys().map(r);\n}\n\nconst Project = ({ id }) => {\n    switch (id) {\n        case \"thirty_day_adulting\":\n            return <ThirtyDayAdulting />;\n        case \"twentyfour\":\n            return <TwentyFour />;\n        case \"sentence_mosaics\":\n            return <SentenceMosaics />;\n        case \"dfa_website\":\n            return <DFAWebsite />;\n        case \"duquesne_incline\":\n            return <DuquesneWebsite />;\n        case \"multitask\":\n            return <Multitask />;\n        case \"fish\":\n            return <Fish />;\n        case \"bit_logo\":\n            return <BitLogo />;\n        case \"communication_design\":\n            return <CommunicationDesign />;\n        case \"microsite\":\n            return <Microsite />;\n        case \"art\":\n            return <Art />;\n        case \"ccc\":\n            return <CrossCountryCommunications />;\n        case \"acrylic_pour_painting_platform\":\n            return<AcrylicPourPaintingPlatform />;\n        case \"textile_work\":\n            return <TextileWork />;\n        default:\n            return <p>{id}</p>;\n    }\n};\n\nexport default ProjectSections;\n","/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/Multitask.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/Fish.js",[],"/Users/ericafu/CMU/erica-w-fu.github.io/src/components/ProjectSection/ProjectMediaComponents/CrossCountryConnections.js",["167","168"],"import React from \"react\";\nimport '../ProjectSection.css';\n\nimport img1 from '../project-media/Cross Country Connections/ccc-erica.jpeg'\nimport img2 from '../project-media/Cross Country Connections/ccc-mot